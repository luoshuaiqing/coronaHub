{
  "name": "coronahub",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.html",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server --port=8050",
    "webpack": "webpack",
    "start": "npm-run-all --parallel devserver watch:sass webpack",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luoshuaiqing/coronaHub.git"
  },
  "author": "Shuaiqing Luo, Lihan Zhu, Tong Zhi, Yun Bao, Yuxiao Hang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luoshuaiqing/coronaHub/issues"
  },
  "homepage": "https://github.com/luoshuaiqing/coronaHub#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "concat": "^1.0.3",
    "css-loader": "^3.4.2",
    "exports-loader": "^0.7.0",
    "node-sass": "^4.13.1",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "gh-pages": "^2.1.1",
    "jquery": "^3.5.0",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1"
  }
}
